ttributes":{"src":"https://api.iconify.design/lucide-chevron-down.svg?color=%231f2937","alt":"Minimize"}},{"type":"textnode","content":" Hide\n        "}]}]}]}]},{"classes":["topic-row"],"attributes":{"id":"medtechTopicRow","role":"group","aria-label":"Quick topics"},"components":[{"type":"button","classes":["topic-chip"],"attributes":{"type":"button","data-topic":"risk-classes"},"components":[{"type":"contentEditable","components":[{"type":"image","resizable":{"ratioDefault":1},"classes":["chip-icon"],"attributes":{"src":"https://api.iconify.design/lucide-shield.svg?color=%230369a1","alt":"Risk classes"}},{"type":"textnode","content":" Risk classes\n      "}]}]},{"type":"button","classes":["topic-chip"],"attributes":{"type":"button","data-topic":"fda-pathways"},"components":[{"type":"contentEditable","components":[{"type":"image","resizable":{"ratioDefault":1},"classes":["chip-icon"],"attributes":{"src":"https://api.iconify.design/lucide-badge-check.svg?color=%230369a1","alt":"FDA pathways"}},{"type":"textnode","content":" FDA pathways\n      "}]}]},{"type":"button","classes":["topic-chip"],"attributes":{"type":"button","data-topic":"business-models"},"components":[{"type":"contentEditable","components":[{"type":"image","resizable":{"ratioDefault":1},"classes":["chip-icon"],"attributes":{"src":"https://api.iconify.design/lucide-layers.svg?color=%230369a1","alt":"Business models"}},{"type":"textnode","content":" Business models\n      "}]}]},{"type":"button","classes":["topic-chip"],"attributes":{"type":"button","data-topic":"stakeholders"},"components":[{"type":"contentEditable","components":[{"type":"image","resizable":{"ratioDefault":1},"classes":["chip-icon"],"attributes":{"src":"https://api.iconify.design/lucide-users.svg?color=%230369a1","alt":"Stakeholders"}},{"type":"textnode","content":" Stakeholders\n      "}]}]},{"type":"button","classes":["topic-chip"],"attributes":{"type":"button","data-topic":"data-strategy"},"components":[{"type":"contentEditable","components":[{"type":"image","resizable":{"ratioDefault":1},"classes":["chip-icon"],"attributes":{"src":"https://api.iconify.design/lucide-database.svg?color=%230369a1","alt":"Data strategy"}},{"type":"textnode","content":" Data strategy\n      "}]}]},{"type":"button","classes":["topic-chip"],"attributes":{"type":"button","data-topic":"how-to-win"},"components":[{"type":"contentEditable","components":[{"type":"image","resizable":{"ratioDefault":1},"classes":["chip-icon"],"attributes":{"src":"https://api.iconify.design/lucide-rocket.svg?color=%230369a1","alt":"How to win"}},{"type":"textnode","content":" How to win\n      "}]}]}]},{"classes":["chat-area"],"attributes":{"id":"medtechChatArea","aria-live":"polite","aria-relevant":"additions"},"components":[{"type":"comment","content":" Messages will appear here "}]},{"type":"form","classes":["input-row"],"attributes":{"method":"get","id":"medtechChatForm","aria-label":"Chat input area"},"components":[{"type":"input","void":true,"classes":["chat-input"],"attributes":{"type":"text","id":"medtechChatInput","name":"message","placeholder":"Type a question and press Enter","aria-label":"Your message"}},{"type":"button","classes":["send-btn"],"attributes":{"type":"submit","id":"medtechSendBtn","aria-label":"Send message"},"components":[{"type":"contentEditable","components":[{"type":"image","resizable":{"ratioDefault":1},"classes":["send-icon"],"attributes":{"src":"https://api.iconify.design/lucide-send.svg?color=%23ffffff","alt":"Send"}},{"type":"textnode","content":" Send\n      "}]}]}]}]},{"type":"script","components":[{"type":"textnode","content":"\n    (function () {\n      const toggleBtn = document.getElementById('medtechBotToggle');\n      const panel = document.getElementById('medtechBotPanel');\n      const minimizeBtn = document.getElementById('medtechMinimizeBtn');\n      const clearBtn = document.getElementById('medtechClearBtn');\n      const chatArea = document.getElementById('medtechChatArea');\n      const form = document.getElementById('medtechChatForm');\n      const input = document.getElementById('medtechChatInput');\n      const topicRow = document.getElementById('medtechTopicRow');\n\n      function scrollToBottom() {\n        if (!chatArea) return;\n        chatArea.scrollTop = chatArea.scrollHeight;\n      }\n\n      function addMessage(text, role) {\n        const msg = document.createElement('div');\n        msg.className = 'msg ' + role;\n        msg.setAttribute('role', 'group');\n        msg.setAttribute('aria-label', role === 'user' ? 'Your message' : 'Assistant message');\n\n        const meta = document.createElement('div');\n        meta.className = 'meta';\n\n        const icon = document.createElement('img');\n        icon.className = 'meta-icon';\n        icon.alt = role === 'user' ? 'You' : 'Assistant';\n        icon.loading = 'lazy';\n        icon.src = role === 'user'\n          ? 'https://api.iconify.design/lucide-user.svg?color=%2364748b'\n          : 'https://api.iconify.design/lucide-bot.svg?color=%2364748b';\n\n        const label = document.createElement('span');\n        label.textContent = role === 'user' ? 'You' : 'Assistant';\n\n        meta.appendChild(icon);\n        meta.appendChild(label);\n\n        const p = document.createElement('p');\n        p.textContent = text;\n\n        msg.appendChild(meta);\n        msg.appendChild(p);\n        chatArea.appendChild(msg);\n        scrollToBottom();\n      }\n\n      function addExpandableBotMessage(heading, items) {\n        const msg = document.createElement('div');\n        msg.className = 'msg bot';\n        msg.setAttribute('role', 'group');\n        msg.setAttribute('aria-label', 'Assistant message');\n\n        const meta = document.createElement('div');\n        meta.className = 'meta';\n\n        const icon = document.createElement('img');\n        icon.className = 'meta-icon';\n        icon.alt = 'Assistant';\n        icon.loading = 'lazy';\n        icon.src = 'https://api.iconify.design/lucide-bot.svg?color=%2364748b';\n\n        const label = document.createElement('span');\n        label.textContent = 'Assistant';\n\n        meta.appendChild(icon);\n        meta.appendChild(label);\n\n        const p = document.createElement('p');\n        p.textContent = heading;\n\n        const details = document.createElement('details');\n        const summary = document.createElement('summary');\n\n        const sumIcon = document.createElement('img');\n        sumIcon.className = 'summary-icon';\n        sumIcon.alt = '';\n        sumIcon.loading = 'lazy';\n        sumIcon.src = 'https://api.iconify.design/lucide-chevron-right.svg?color=%230f172a';\n\n        summary.appendChild(sumIcon);\n        summary.appendChild(document.createTextNode('More details'));\n\n        const list = document.createElement('ul');\n        list.style.marginTop = '0.5rem';\n        list.style.paddingLeft = '1.25rem';\n        list.style.listStyleType = 'disc';\n\n        items.forEach(t => {\n          const li = document.createElement('li');\n          li.textContent = t;\n          list.appendChild(li);\n        });\n\n        details.appendChild(summary);\n        details.appendChild(list);\n\n        msg.appendChild(meta);\n        msg.appendChild(p);\n        msg.appendChild(details);\n        chatArea.appendChild(msg);\n        scrollToBottom();\n      }\n\n      function reply(text) {\n        const q = (text || '').toLowerCase();\n\n        if (q.includes('class i') || q.includes('class ii') || q.includes('class iii') || q.includes('risk class') || q === 'risk-classes') {\n          addExpandableBotMessage(\n            'FDA risk classes organize devices by patient risk and evidence burden.',\n            [\n              'Class I: Low risk; least regulatory burden.',\n              'Class II: Moderate risk; usually 510(k) with special controls.',\n              'Class III: High risk/implantable; typically PMA with rigorous evidence.'\n            ]\n          );\n          return;\n        }\n        if (q.includes('510(k)') || q.includes('pma') || q.includes('de novo') || q.includes('hde') || q.includes('eua') || q.includes('fda') || q === 'fda-pathways') {\n          addExpandableBotMessage(\n            'Key FDA premarket pathways differ in novelty, risk, and evidence requirements.',\n            [\n              '510(k): Substantial equivalence to a predicate for moderaterisk devices.',\n              'De novo: Novel lowmoderate risk devices without a predicate.',\n              'PMA: Full premarket approval for highrisk Class III devices.',\n              'HDE: For rare diseases; limited evidence with safeguards.',\n              'EUA: Temporary authorization during emergencies.'\n            ]\n          );\n          return;\n        }\n        if (q.includes('business model') || q.includes('recurring') || q.includes('saas') || q.includes('managed service') || q.includes('disposable') || q.includes('ai') || q === 'business-models') {\n          addExpandableBotMessage(\n            'Modern medtech blends hardware, software, and data into recurring value.',\n            [\n              'Capital + disposables: Upfront platform + ongoing consumables.',\n              'Managed service / payperuse: Aligns cost with usage.',\n              'SaaS / AI decision support: Subscription software layered on devices.',\n              'Data monetization: Deidentified realworld data for insights and value.'\n            ]\n          );\n          return;\n        }\n        if (q.includes('stakeholder') || q.includes('payer') || q.includes('vac') || q.includes('clinician') || q.includes('administrator') || q.includes('patient') || q.includes('economics') || q === 'stakeholders') {\n          addExpandableBotMessage(\n            'Understand each stakeholders incentives to accelerate adoption.',\n            [\n              'Clinicians: Outcomes, usability, workflow fit.',\n              'Administrators/VACs: Total cost of care, operational impact, lowrisk implementation.',\n              'Payers: Better outcomes per dollar; valuebased alignment.',\n              'Regulators: Safety, effectiveness, lifecycle risk controls.',\n              'Patients: Access, experience, transparency and trust in data use.'\n            ]\n          );\n          return;\n        }\n        if (q.includes('data') || q.includes('post-market') || q.includes('real-world') || q.includes('evidence') || q === 'data-strategy') {\n          addExpandableBotMessage(\n            'Postmarket, realworld data is a strategic asset across the lifecycle.',\n            [\n              'Capture performance and outcomes to prove value.',\n              'Iterate product and workflow based on utilization.',\n              'Support pricing, reimbursement, and valuebased care.',\n              'Open new lines of business around insights.'\n            ]\n          );\n          return;\n        }\n        if ((q.includes('how') && (q.includes('win') || q.includes('succeed') || q.includes('adopt'))) || q === 'how-to-win') {\n          addExpandableBotMessage(\n            'A practical playbook for winning medtech adoption:',\n            [\n              'Prove value with highquality clinical + realworld evidence.',\n              'Turn devices into platforms with software/AI/services.',\n              'Align incentives for clinicians, administrators, payers, and patients.',\n              'Treat data as a strategic asset across the lifecycle.'\n            ]\n          );\n          return;\n        }\n\n        addMessage(\n          'Great question! Ask me about medtech strategy, FDA pathways, business models, stakeholders, or datacentric value. Try:\\n Whats the difference between 510(k) and PMA?\\n Which stakeholders influence hospital adoption?\\n How do managed service models work?',\n          'bot'\n        );\n      }\n\n      function openPanel() {\n        panel.classList.add('open');\n        panel.style.opacity = '0';\n        panel.style.transform = 'translateY(8px)';\n        requestAnimationFrame(() => {\n          panel.style.opacity = '1';\n          panel.style.transform = 'translateY(0)';\n        });\n        toggleBtn.setAttribute('aria-expanded', 'true');\n        input.focus();\n      }\n      function closePanel() {\n        panel.style.opacity = '0';\n        panel.style.transform = 'translateY(8px)';\n        setTimeout(() => {\n          panel.classList.remove('open');\n          panel.style.opacity = '';\n          panel.style.transform = '';\n          toggleBtn.setAttribute('aria-expanded', 'false');\n        }, 160);\n      }\n\n      // Toggle open\n      toggleBtn.addEventListener('click', () => {\n        if (panel.classList.contains('open')) {\n          closePanel();\n        } else {\n          openPanel();\n        }\n      });\n\n      // Minimize\n      minimizeBtn.addEventListener('click', () => {\n        if (panel.classList.contains('open')) {\n          minimizeBtn.innerHTML = '<img src=\"https://api.iconify.design/lucide-chevron-up.svg?color=%231f2937\" alt=\"Show\" class=\"send-icon\"> Show';\n          minimizeBtn.setAttribute('aria-label', 'Expand chatbot');\n          minimizeBtn.setAttribute('aria-expanded', 'false');\n          closePanel();\n        } else {\n          minimizeBtn.innerHTML = '<img src=\"https://api.iconify.design/lucide-chevron-down.svg?color=%231f2937\" alt=\"Hide\" class=\"send-icon\"> Hide';\n          minimizeBtn.setAttribute('aria-label', 'Minimize chatbot');\n          minimizeBtn.setAttribute('aria-expanded', 'true');\n          openPanel();\n        }\n      });\n\n      // Clear chat\n      clearBtn.addEventListener('click', () => {\n        chatArea.innerHTML = '';\n        addMessage('Hi! I can help you explore medtech strategy topics. Pick a chip above or ask a question.', 'bot');\n      });\n\n      // Form submit and Enter key\n      form.addEventListener('submit', (e) => {\n        e.preventDefault();\n        const text = (input.value || '').trim();\n        if (!text) return;\n        addMessage(text, 'user');\n        input.value = '';\n        setTimeout(() => reply(text), 200);\n      });\n      input.addEventListener('keydown', (e) => {\n        if (e.key === 'Enter' && !e.shiftKey) {\n          e.preventDefault();\n          form.dispatchEvent(new Event('submit'));\n        }\n      });\n\n      // Topic chips\n      topicRow.addEventListener('click', (e) => {\n        const chip = e.target.closest('.topic-chip');\n        if (!chip) return;\n        const topic = chip.getAttribute('data-topic');\n        addMessage(chip.textContent.trim(), 'user');\n        setTimeout(() => reply(topic), 150);\n      });\n\n      // Initialize with a friendly message\n      addMessage('Welcome! Ask me about risk classes, FDA pathways, business models, stakeholders, and data strategy in medtech.', 'bot');\n    })();\n  "}]}]}],"doctype":"<!DOCTYPE html>","head":{"type":"head","components":[{"tagName":"meta","name":"Meta Charset","void":true,"attributes":{"charset":"utf-8"}},{"tagName":"meta","name":"Meta Viewport","void":true,"attributes":{"name":"viewport","content":"width=device-width, initial-scale=1.0"}},{"tagName":"meta","name":"Meta Theme","void":true,"attributes":{"name":"theme-color","content":"#0ea5e9"}},{"tagName":"title","type":"text","name":"Document Title","attributes":{"id":"i4wl39g"},"components":[{"type":"textnode","content":"From Devices to Data Platforms  Medtech Strategy in a HighStakes Market"}]},{"tagName":"link","name":"Google Fonts Preconnect","void":true,"attributes":{"rel":"preconnect","href":"https://fonts.googleapis.com"}},{"tagName":"link","name":"Google Fonts Preconnect 2","void":true,"attributes":{"rel":"preconnect","href":"https://fonts.gstatic.com","crossorigin":""}},{"tagName":"link","name":"Google Fonts Families","void":true,"attributes":{"href":"https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700&family=Playfair+Display:wght@6