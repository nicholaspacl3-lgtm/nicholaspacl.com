ibutes":{"src":"https://api.iconify.design/lucide-shield-check.svg?color=%230369a1","alt":"Face-saving"}},{"type":"textnode","content":"\n        Face-saving\n      "}]}]},{"type":"button","classes":["topic-chip"],"attributes":{"type":"button","data-topic":"meetings"},"components":[{"type":"contentEditable","components":[{"type":"image","resizable":{"ratioDefault":1},"classes":["chip-icon"],"attributes":{"src":"https://api.iconify.design/lucide-calendar.svg?color=%230369a1","alt":"Meeting etiquette"}},{"type":"textnode","content":"\n        Meeting etiquette\n      "}]}]}]},{"classes":["chat-area"],"attributes":{"id":"twChatArea"},"components":[{"classes":["msg","bot"],"components":[{"classes":["meta"],"components":[{"type":"image","resizable":{"ratioDefault":1},"classes":["meta-icon"],"attributes":{"src":"https://api.iconify.design/lucide-bot.svg?color=%2364778b","alt":"Bot"}},{"type":"textnode","content":"\n          Taiwan Culture Assistant\n        "}]},{"type":"textnode","content":"\n        Hi! Ask me anything about Taiwans business culturerelationships, hierarchy, face-saving, and meeting etiquette. Choose a topic above or type your question.\n        "},{"tagName":"details","components":[{"tagName":"summary","components":[{"type":"image","resizable":{"ratioDefault":1},"classes":["summary-icon"],"attributes":{"src":"https://api.iconify.design/lucide-info.svg?color=%230f172a","alt":"Info"}},{"type":"textnode","content":"\n            What I can help with\n          "}]},{"tagName":"ul","attributes":{"id":"ixg9d8w"},"components":[{"tagName":"li","type":"text","attributes":{"building":"","trust":"","and":"","propriety":"","(xn":"","l)<":"","li":""},"components":[{"type":"textnode","content":"\n            "}]},{"tagName":"li","type":"text","attributes":{"navigating":"","seniority":"","and":"","approvals<":"","li":""},"components":[{"type":"textnode","content":"\n            "}]},{"tagName":"li","type":"text","attributes":{"indirect":"","communication":"","and":"","protecting":"","face<":"","li":""},"components":[{"type":"textnode","content":"\n            "}]},{"tagName":"li","type":"text","attributes":{"preparing":"","agendas,":"","docs,":"","and":"","follow-ups<":"","li":""},"components":[{"type":"textnode","content":"\n          "}]}]}]}]}]},{"classes":["input-row"],"components":[{"type":"input","void":true,"classes":["chat-input"],"attributes":{"type":"text","id":"twChatInput","placeholder":"Type your question"}},{"type":"button","classes":["send-btn"],"attributes":{"type":"button","id":"twSendBtn","aria-label":"Send message"},"components":[{"type":"contentEditable","components":[{"type":"image","resizable":{"ratioDefault":1},"classes":["send-icon"],"attributes":{"src":"https://api.iconify.design/lucide-send.svg?color=%23ffffff","alt":"Send"}},{"type":"textnode","content":"\n        Send\n      "}]}]}]}]},{"type":"script","components":[{"type":"textnode","content":"\n    (function(){\n      const panel = document.getElementById('twBotPanel');\n      const toggle = document.getElementById('twBotToggle');\n      const closeBtn = document.getElementById('twBotClose');\n      const minimizeBtn = document.getElementById('twBotMinimize');\n      const chatArea = document.getElementById('twChatArea');\n      const input = document.getElementById('twChatInput');\n      const sendBtn = document.getElementById('twSendBtn');\n      const topicRow = document.getElementById('twTopicRow');\n\n      function openPanel() {\n        panel.classList.add('open');\n        input.focus();\n      }\n      function closePanel() {\n        panel.classList.remove('open');\n      }\n      function minimizePanel() {\n        panel.classList.toggle('open');\n      }\n\n      toggle.addEventListener('click', openPanel);\n      closeBtn.addEventListener('click', closePanel);\n      minimizeBtn.addEventListener('click', minimizePanel);\n\n      function addMessage(text, sender = 'user', detailsHtml) {\n        const wrapper = document.createElement('div');\n        wrapper.className = 'msg ' + (sender === 'user' ? 'user' : 'bot');\n        if (sender === 'bot') {\n          const meta = document.createElement('div');\n          meta.className = 'meta';\n          meta.innerHTML = '<img class=\"meta-icon\" src=\"https://api.iconify.design/lucide-bot.svg?color=%2364778b\" alt=\"Bot\"> Taiwan Culture Assistant';\n          wrapper.appendChild(meta);\n        }\n        const p = document.createElement('div');\n        p.textContent = text;\n        wrapper.appendChild(p);\n\n        if (detailsHtml && sender === 'bot') {\n          const details = document.createElement('details');\n          const summary = document.createElement('summary');\n          summary.innerHTML = '<img class=\"summary-icon\" src=\"https://api.iconify.design/lucide-chevron-down.svg?color=%230f172a\" alt=\"More\"> Details';\n          details.appendChild(summary);\n          const content = document.createElement('div');\n          content.innerHTML = detailsHtml;\n          details.appendChild(content);\n          wrapper.appendChild(details);\n        }\n\n        chatArea.appendChild(wrapper);\n        chatArea.scrollTop = chatArea.scrollHeight;\n      }\n\n      function answerForTopic(topic) {\n        switch (topic) {\n          case 'introductions':\n            addMessage(\n              'Introductions prioritize seniority and propriety (L). Let the most senior host open, use formal titles, exchange cards with both hands, and offer concise context.',\n              'bot',\n              '<ul style=\"margin-top:0.5rem; padding-left:1.25rem;\">' +\n              '<li Stand, smile, and make light eye contact.</li>' +\n              '<li Present your card with both hands; receive theirs the same way and take a moment to read it.</li>' +\n              '<li Title first, name second (e.g., Director Lin).</li>' +\n              '<li Keep your intro short: role, purpose, and how you support the group.</li>' +\n              '</ul>'\n            );\n            break;\n          case 'hierarchy':\n            addMessage(\n              'Seniority shapes flow: who speaks, seating, and decision paths. Map the approval chain and keep leaders informed with crisp documentation.',\n              'bot',\n              '<ul style=\"margin-top:0.5rem; padding-left:1.25rem;\">' +\n              '<li Send agendas in advance and confirm owner/approver.</li>' +\n              '<li Escalate respectfullysummarize options and trade-offs.</li>' +\n              '<li Acknowledge contributions publicly; give corrective feedback privately.</li>' +\n              '</ul>'\n            );\n            break;\n          case 'face':\n            addMessage(\n              'Face-saving protects relationships. Use indirect language, invite perspectives, and frame feedback around shared goals.',\n              'bot',\n              '<ul style=\"margin-top:0.5rem; padding-left:1.25rem;\">' +\n              '<li Prefer Perhaps we could over direct You should.</li>' +\n              '<li Offer alternatives instead of outright rejection.</li>' +\n              '<li Attribute wins to the group; own mistakes personally.</li>' +\n              '</ul>'\n            );\n            break;\n          case 'meetings':\n            addMessage(\n              'Meetings reward preparation and precision. Clear agendas, time-bound items, and diligent follow-ups make decisions travel faster.',\n              'bot',\n              '<ul style=\"margin-top:0.5rem; padding-left:1.25rem;\">' +\n              '<li Agenda: objectives, owners, and timing per item.</li>' +\n              '<li Minutes: decisions, action items, owners, due dates.</li>' +\n              '<li Follow-up within 2448 hours; keep senior stakeholders in the loop.</li>' +\n              '</ul>'\n            );\n            break;\n          default:\n            addMessage('I can help with introductions, hierarchy, face-saving, and meeting etiquette. What would you like to explore?', 'bot');\n        }\n      }\n\n      topicRow.addEventListener('click', function(e){\n        const chip = e.target.closest('.topic-chip');\n        if (!chip) return;\n        const topic = chip.getAttribute('data-topic');\n        addMessage(chip.textContent.trim(), 'user');\n        setTimeout(() => answerForTopic(topic), 300);\n      });\n\n      function generateAnswer(q) {\n        const text = q.toLowerCase();\n        if (text.includes('introduc') || text.includes('greet') || text.includes('card')) {\n          answerForTopic('introductions');\n          return;\n        }\n        if (text.includes('hierarch') || text.includes('senior') || text.includes('approval')) {\n          answerForTopic('hierarchy');\n          return;\n        }\n        if (text.includes('face') || text.includes('indirect') || text.includes('polite')) {\n          answerForTopic('face');\n          return;\n        }\n        if (text.includes('meeting') || text.includes('agenda') || text.includes('follow')) {\n          answerForTopic('meetings');\n          return;\n        }\n        // General guidance from the article\n        addMessage(\n          'Taiwans business culture blends relational ethics with disciplined process. Build trust patiently (Xn), act with propriety (L), respect hierarchy, and communicate with subtlety.',\n          'bot',\n          '<ul style=\"margin-top:0.5rem; padding-left:1.25rem;\">' +\n          '<li Invest in relationshipssmall, consistent commitments.</li>' +\n          '<li Prepare thoroughlyclear agendas and crisp docs.</li>' +\n          '<li Protect face when giving feedback; prefer collaborative framing.</li>' +\n          '<li Move with w wi: align with existing currents for sustainable momentum.</li>' +\n          '</ul>'\n        );\n      }\n\n      function handleSend() {\n        const val = input.value.trim();\n        if (!val) return;\n        addMessage(val, 'user');\n        input.value = '';\n        setTimeout(() => generateAnswer(val), 350);\n      }\n\n      sendBtn.addEventListener('click', handleSend);\n      input.addEventListener('keydown', function(e){\n        if (e.key === 'Enter') handleSend();\n      });\n    })();\n  "}]}]},{"tagName":"footer","name":"Footer","classes":["footer"],"components":[{"name":"Footer Container","classes":["footer-container"],"attributes":{"id":"iixlnvy"},"components":[{"name":"Footer Top Row","classes":["footer-top-row"],"components":[{"name":"Footer Brand","classes":["footer-brand"],"components":[{"name":"Footer Brand Row","classes":["footer-brand-row"],"components":[{"name":"Footer Brand Mark","classes":["footer-brand-mark"],"components":[{"type":"image","name":"Footer Brand Icon","resizable":{"ratioDefault":1},"classes":["footer-brand-icon"],"attributes":{"alt":"Logo","src":"https://api.iconify.design/lucide-compass.svg?color=%230ea5e9"}}]},{"tagName":"span","type":"text","name":"Footer Brand Name","classes":["footer-brand-name"],"attributes":{"id":"iit1hzi"},"components":[{"type":"textnode","content":"Nicholas Pacl"}]}]}]}]},{"name":"Footer Divider","classes":["footer-divider"]},{"name":"Footer Bottom","classes":["footer-bottom"],"components":[{"tagName":"p","type":"text","name":"Footer Copyright","classes":["footer-copyright"],"attributes":{"id":"i3dnmli"},"components":[{"type":"textnode","content":" 2025 Nicholas Pacl All rights reserved."}]},{"name":"Footer Social","classes":["footer-social"]}]}]}]}],"doctype":"<!DOCTYPE html>","head":{"type":"head","name":"Head","components":[{"tagName":"meta","name":"Meta Charset","void":true,"attributes":{"charset":"utf-8"}},{"tagName":"meta","name":"Meta Viewport","void":true,"attributes":{"name":"viewport","content":"width=device-width, initial-scale=1"}},{"tagName":"title","type":"text","name":"Title","attributes":{"id":"ix51l4e"},"components":[{"type":"textnode","content":"Navigating the Dynamic Matrix: Understanding Taiwan's Unique Business Culture  Blog Article"}]},{"tagName":"meta","name":"Meta 