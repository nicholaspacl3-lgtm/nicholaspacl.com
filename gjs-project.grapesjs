aria-label":"Quantum Computing Chatbot Agent","aria-modal":"false"},"components":[{"classes":["panel-header"],"components":[{"classes":["header-left"],"components":[{"tagName":"span","classes":["bot-badge"],"components":[{"type":"image","resizable":{"ratioDefault":1},"classes":["bot-icon"],"attributes":{"src":"https://api.iconify.design/lucide-orbit.svg?color=%230ea5e9","alt":"Quantum assistant badge"}}]},{"type":"text","classes":["bot-title"],"components":[{"type":"textnode","content":"Quantum Computing Chatbot Agent"}]}]},{"classes":["header-actions"],"components":[{"type":"button","classes":["header-button"],"attributes":{"type":"button","id":"qcMinBtn","aria-label":"Minimize"},"components":[{"type":"contentEditable","components":[{"type":"image","resizable":{"ratioDefault":1},"classes":["fab-icon"],"attributes":{"src":"https://api.iconify.design/lucide-chevron-down.svg?color=currentColor","alt":"Minimize"}},{"tagName":"span","type":"text","components":[{"type":"textnode","content":"Minimize"}]}]}]},{"type":"button","classes":["header-button"],"attributes":{"type":"button","id":"qcCloseBtn","aria-label":"Close"},"components":[{"type":"contentEditable","components":[{"type":"image","resizable":{"ratioDefault":1},"classes":["fab-icon"],"attributes":{"src":"https://api.iconify.design/lucide-x.svg?color=currentColor","alt":"Close chatbot"}},{"tagName":"span","type":"text","components":[{"type":"textnode","content":"Close"}]}]}]}]}]},{"classes":["topic-row"],"attributes":{"aria-label":"Quick topics"},"components":[{"type":"button","classes":["topic-chip"],"attributes":{"type":"button","data-topic":"qubits"},"components":[{"type":"contentEditable","components":[{"type":"image","resizable":{"ratioDefault":1},"classes":["chip-icon"],"attributes":{"src":"https://api.iconify.design/lucide-atom.svg?color=%230ea5e9","alt":"Qubits"}},{"tagName":"span","type":"text","components":[{"type":"textnode","content":"Qubits"}]}]}]},{"type":"button","classes":["topic-chip"],"attributes":{"type":"button","data-topic":"superposition"},"components":[{"type":"contentEditable","components":[{"type":"image","resizable":{"ratioDefault":1},"classes":["chip-icon"],"attributes":{"src":"https://api.iconify.design/lucide-layers.svg?color=%230ea5e9","alt":"Superposition"}},{"tagName":"span","type":"text","components":[{"type":"textnode","content":"Superposition"}]}]}]},{"type":"button","classes":["topic-chip"],"attributes":{"type":"button","data-topic":"entanglement"},"components":[{"type":"contentEditable","components":[{"type":"image","resizable":{"ratioDefault":1},"classes":["chip-icon"],"attributes":{"src":"https://api.iconify.design/lucide-link.svg?color=%230ea5e9","alt":"Entanglement"}},{"tagName":"span","type":"text","components":[{"type":"textnode","content":"Entanglement"}]}]}]},{"type":"button","classes":["topic-chip"],"attributes":{"type":"button","data-topic":"noise"},"components":[{"type":"contentEditable","components":[{"type":"image","resizable":{"ratioDefault":1},"classes":["chip-icon"],"attributes":{"src":"https://api.iconify.design/lucide-waveform.svg?color=%230ea5e9","alt":"Noise and errors"}},{"tagName":"span","type":"text","components":[{"type":"textnode","content":"Noise & Errors"}]}]}]},{"type":"button","classes":["topic-chip"],"attributes":{"type":"button","data-topic":"pqc"},"components":[{"type":"contentEditable","components":[{"type":"image","resizable":{"ratioDefault":1},"classes":["chip-icon"],"attributes":{"src":"https://api.iconify.design/lucide-shield-check.svg?color=%230ea5e9","alt":"Post-quantum cryptography"}},{"tagName":"span","type":"text","components":[{"type":"textnode","content":"Security (PQC)"}]}]}]},{"type":"button","classes":["topic-chip"],"attributes":{"type":"button","data-topic":"try-ibm"},"components":[{"type":"contentEditable","components":[{"type":"image","resizable":{"ratioDefault":1},"classes":["chip-icon"],"attributes":{"src":"https://api.iconify.design/lucide-cloud.svg?color=%230ea5e9","alt":"Try quantum in the cloud"}},{"tagName":"span","type":"text","components":[{"type":"textnode","content":"Try Quantum"}]}]}]}]},{"classes":["chat-area"],"attributes":{"id":"qcChatArea"},"components":[{"classes":["msg","bot"],"components":[{"classes":["meta"],"components":[{"type":"image","resizable":{"ratioDefault":1},"classes":["meta-icon"],"attributes":{"src":"https://api.iconify.design/lucide-sparkles.svg?color=currentColor","alt":"Assistant"}},{"tagName":"span","type":"text","components":[{"type":"textnode","content":"Hi! Im your quantum learning guide. Ask a question or tap a topic chip to explore."}]}]},{"tagName":"p","type":"text","components":[{"type":"textnode","content":"Curious about how qubits differ from bits, why noise matters, or how postquantum cryptography keeps data safe? Ill keep things in plain English and link it back to the article."}]},{"tagName":"details","components":[{"tagName":"summary","components":[{"type":"image","resizable":{"ratioDefault":1},"classes":["summary-icon"],"attributes":{"src":"https://api.iconify.design/lucide-chevron-right.svg?color=currentColor","alt":""}},{"type":"textnode","content":"\n            What I can help with\n          "}]},{"tagName":"ul","attributes":{"id":"i85hkit"},"components":[{"tagName":"li","type":"text","components":[{"type":"textnode","content":"Core concepts: qubits, superposition, entanglement"}]},{"tagName":"li","type":"text","components":[{"type":"textnode","content":"Current limits: noise, error rates, NISQ era"}]},{"tagName":"li","type":"text","components":[{"type":"textnode","content":"Use cases: molecules, logistics, AI+quantum"}]},{"tagName":"li","type":"text","components":[{"type":"textnode","content":"Security: postquantum cryptography basics"}]},{"tagName":"li","type":"text","components":[{"type":"textnode","content":"Handson: trying cloud quantum platforms"}]}]}]}]}]},{"classes":["input-row"],"components":[{"type":"input","void":true,"classes":["chat-input"],"attributes":{"type":"text","id":"qcInput","placeholder":"Ask in plain English, e.g., 'Explain entanglement like Im 12'"}},{"type":"button","classes":["send-btn","gjs-t-button"],"attributes":{"type":"button","id":"qcSend"},"components":[{"type":"contentEditable","components":[{"type":"image","resizable":{"ratioDefault":1},"classes":["send-icon"],"attributes":{"src":"https://api.iconify.design/lucide-send.svg?color=currentColor","alt":"Send"}},{"tagName":"span","type":"text","components":[{"type":"textnode","content":"Send"}]}]}]}]}]},{"type":"script","components":[{"type":"textnode","content":"\n    (function(){\n      const panel = document.getElementById('qc-agent-panel');\n      const openBtn = document.getElementById('qcOpenBtn');\n      const closeBtn = document.getElementById('qcCloseBtn');\n      const minBtn = document.getElementById('qcMinBtn');\n      const chat = document.getElementById('qcChatArea');\n      const input = document.getElementById('qcInput');\n      const send = document.getElementById('qcSend');\n      const chips = document.querySelectorAll('#quantumLearningBot .topic-chip');\n\n      const topics = {\n        qubits: {\n          title: 'Qubits in plain English',\n          body: 'Bits are tiny on/off switches: 0 or 1. Qubits can be 0 and 1 at the same time (superposition). That lets quantum computers explore many possibilities in parallel, which helps on specific problemsnot general tasks.',\n          expand: 'Think of a coin: a classical bit is heads or tails resting on a table. A qubit is like a coin spinning in the airboth possibilities at once until you measure it.'\n        },\n        superposition: {\n          title: 'Superposition',\n          body: 'Superposition means a qubit can be in a mix of 0 and 1 simultaneously. Its useful for building quantum states that encode many options at once.',\n          expand: 'You benefit only if you later use interference to amplify good answers and cancel bad onesotherwise the parallelism doesnt help when measured.'\n        },\n        entanglement: {\n          title: 'Entanglement',\n          body: 'Entanglement links qubits so strongly that measuring one influences the other, no matter how far apart. Its a resource that powers quantum speedups and secure protocols.',\n          expand: 'In practice, entanglement lets algorithms coordinate across qubits, creating correlations classical bits cannot mimic efficiently.'\n        },\n        noise: {\n          title: 'Noise & errors (NISQ era)',\n          body: 'Todays devices have tens to hundreds of qubits but are noisy. Heat, vibrations, and electronics disturb qubits, causing errors. Thats why we use short circuits, error mitigation, and hybrid (quantum+classical) approaches.',\n          expand: 'Engineers fight noise with cryogenics, shielding, and better qubit designs. Error correction will require many physical qubits for each logical qubit.'\n        },\n        pqc: {\n          title: 'Security and PostQuantum Cryptography',\n          body: 'Powerful future quantum computers could break widely used encryption (like RSA). Postquantum cryptography uses new math (latticebased, codebased, etc.) designed to resist quantum attacks and is being standardized.',\n          expand: 'Organizations should inventory cryptography now and plan migrations to PQC algorithms to avoid harvest now, decrypt later risks.'\n        },\n        'try-ibm': {\n          title: 'Try quantum in the cloud',\n          body: 'Platforms like IBM Quantum let you run small circuits on real devices via the web, often with free monthly quotas. Start with tutorials, build basic gates, and see noise effects firsthand.',\n          expand: 'Begin with constructing superposition (Hadamard gate) and simple interference patterns, then try Bell states to see entanglement.'\n        }\n      };\n\n      function togglePanel(forceOpen) {\n        const isOpen = panel.classList.contains('open');\n        if (forceOpen === true || (!isOpen)) {\n          panel.classList.add('open');\n          openBtn.setAttribute('aria-expanded', 'true');\n          openBtn.style.display = 'none';\n          input.focus();\n        } else {\n          panel.classList.remove('open');\n          openBtn.setAttribute('aria-expanded', 'false');\n          openBtn.style.display = 'inline-flex';\n        }\n      }\n\n      function minimizePanel() {\n        panel.classList.remove('open');\n        openBtn.setAttribute('aria-expanded', 'false');\n        openBtn.style.display = 'inline-flex';\n      }\n\n      function addMessage(text, role = 'bot', extraHTML = '') {\n        const wrap = document.createElement('div');\n        wrap.className = 'msg ' + role;\n        if (role === 'bot') {\n          const meta = document.createElement('div');\n          meta.className = 'meta';\n          meta.innerHTML = '<img src=\"https://api.iconify.design/lucide-sparkles.svg?color=currentColor\" alt=\"Assistant\" class=\"meta-icon\"><span>Assistant</span>';\n          wrap.appendChild(meta);\n        } else {\n          const meta = document.createElement('div');\n          meta.className = 'meta';\n          meta.innerHTML = '<img src=\"https://api.iconify.design/lucide-user.svg?color=currentColor\" alt=\"You\" class=\"meta-icon\"><span>You</span>';\n          wrap.appendChild(meta);\n        }\n        const p = document.createElement('p');\n        p.textContent = text;\n        wrap.appendChild(p);\n        if (extraHTML) {\n          const details = document.createElement('details');\n          const summary = document.createElement('summary');\n          summary.innerHTML = '<img src=\"https://api.iconify.design/lucide-chevron-right.svg?color=currentColor\" alt=\"\" class=\"summary-icon\"> Learn more';\n          const div = document.createElement('div');\n          div.style.marginTop = '0.5rem';\n          div.style.color = '#334155';\n          div.innerHTML = extraHTML;\n          details.appendChild(summary);\n          details.appendChild(div);\n          wrap.appendChild(details);\n        }\n        chat.appendChild(wrap);\n        chat.scrollTop = chat.scrollHeight;\n      }\n\n      function handleTopic(topicKey) {\n        const t = topics[topicKey];\n        if (!t) return;\n        addMessage(t.title, 'bot', '<p>' + t.expand + '</p>');\n        addMessage(t.body, 'bot');\n      }\n\n      function respondToUser(query) {\n        const q = query.trim().toLowerCase();\n        // Simple intent routing\n        if (q.includes('qubit') || q.includes('bit')) return handleTopic('qubits');\n        if (q.includes('superposition')) return handleTopic('superposition');\n        if (q.includes('entanglement')) return handleTopic('entanglement');\n        if (q.includes('noise') || q.includes('error')) return handleTopic('noise');\n        if (q.includes('security') || q.includes('pqc') || q.includes('cryptography')) return handleTopic('pqc');\n        if (q.includes('try') || q.includes('cloud') || q.includes('ibm')) return handleTopic('try-ibm');\n\n        // Default helpful reply\n        addMessage('Heres a quick way to think about it:', 'bot', '<ul style=\"list-style:disc; padding-left:1rem;\"><li>Identify the concept (e.g., superposition).</li><li>Use a tangible analogy.</li><li>Link to where it shows up in algorithms or hardware.</li></ul>');\n        addMessage('Ask me about superposition vs entanglement, why errors happen, or how to try a real quantum device.', 'bot');\n      }\n\n      openBtn.addEventListener('click', () => togglePanel(true));\n      closeBtn.addEventListener('click', () => togglePanel(false));\n      minBtn.addEventListener('click', minimizePanel);\n\n      chips.forEach(chip => {\n        chip.addEventListener('click', () => {\n          togglePanel(true);\n          const topic = chip.getAttribute('data-topic');\n          addMessage(chip.textContent.trim(), 'user');\n          handleTopic(topic);\n        });\n      });\n\n      send.addEventListener('click', () => {\n        const val = input.value.trim();\n        if (!val) return;\n        addMessage(val, 'user');\n        input.value = '';\n        setTimeout(() => respondToUser(val), 200);\n      });\n\n      input.addEventListener('keydown', (e) => {\n        if (e.key === 'Enter') {\n          e.preventDefault();\n          send.click();\n        }\n      });\n    })();\n  "}]}]}],"doctype":"<!DOCTYPE html>","head":{"type":"head","name":"Head","components":[{"tagName":"meta","name":"Meta Charset","void":true,"attributes":{"charset":"utf-8"}},{"tagName":"meta","name":"Meta Viewport","void":true,"attributes":{"name":"viewport","content":"width=device-width, initial-scale=1"}},{"tagName":"title","type":"text","name":"Document Title","components":[{"type":"textnode","content":"Quantum Computing in Plain English: A Beginners Guide to the Future of Computing  Blog Article"}]},{"tagName":"meta","name":"Meta Description","void":true,"attributes":{"name":"description","content":"A clear, beginner-friendly guide to quantum computing, how it works today, where it could be a game changer, and what comes next."}},{"tagName":"link","name":"Google Fonts","void":true,"attributes":{"href":"https://fonts.googleapis.com/css2?family=Plus+Jakarta+Sans:wght@500;700&family=Inter:wght@400;500;600&display=swap","rel":"stylesheet"}}]},"docEl":{"tagName":"html","attributes":{"lang":"en"}}},"id":"DQXKm7QZs5m3sgpv","minHeight":"500px"}],"id":"q4uTKCcwq2S1aun4gY89QI2Xk"},{"name":"Blog Homepage","frames":[{"component":{"type":"wrapper","name":"Body Root","classes":["gjs-t-body","body-root"],"attributes":{"id":"iplr4el"},"components":[{"tagName":"header","name":"Navbar Section","classes":["navbar-section"],"attributes":{"id":"i2g9ybr"},"components":[{"name":"Header Container","classes":["header-container"],"attributes":{"id":"ibitr-10"},"components":[{"type":"link","name":"Brand Link","editable":false,"classes":["group","brand-link"],"attributes":{"href":"./index.html","id":"id8u3-10"},"components":[{"classes":["brand-ma