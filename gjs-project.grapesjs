ses":["icon-close"],"attributes":{"src":"https://api.iconify.design/lucide-x.svg?color=%230f172a","alt":""}}]}]},{"type":"image","name":"Lightbox Image","resizable":{"ratioDefault":1},"classes":["lightbox-image"],"attributes":{"id":"lightboxImage","src":"data:image/svg+xml;base64,","alt":""}}]},{"type":"script","name":"Page Scripts","components":[{"type":"textnode","content":"\n      const cards = document.querySelectorAll('[data-gjs-name=\"Album Card\"]');\n      const lightbox = document.getElementById('lightbox');\n      const lightboxImg = document.getElementById('lightboxImage');\n      const lightboxClose = document.getElementById('lightboxClose');\n\n      function openLightbox(src, alt) {\n        lightboxImg.src = src;\n        lightboxImg.alt = alt || '';\n        lightbox.classList.remove('hidden');\n        lightbox.classList.add('flex');\n        document.body.style.overflow = 'hidden';\n      }\n      function closeLightbox() {\n        lightbox.classList.add('hidden');\n        lightbox.classList.remove('flex');\n        lightboxImg.src = '';\n        document.body.style.overflow = '';\n      }\n\n      cards.forEach(card => {\n        card.addEventListener('click', (e) => {\n          e.preventDefault();\n          const full = card.getAttribute('data-full-src');\n          const img = card.querySelector('img');\n          openLightbox(full || img.src, img.alt);\n        });\n      });\n      lightbox.addEventListener('click', (e) => {\n        if (e.target === lightbox) closeLightbox();\n      });\n      lightboxClose.addEventListener('click', closeLightbox);\n      document.addEventListener('keydown', (e) => {\n        if (e.key === 'Escape') closeLightbox();\n      });\n\n      document.getElementById('yearSpan').textContent = new Date().getFullYear();\n\n      const copyBtn = document.getElementById('copyAlbumLink');\n      const copyState = document.getElementById('copyAlbumLinkState');\n      copyBtn.addEventListener('click', async () => {\n        const link = 'page://FnKmWMNyqS12TXuzZB0R5laFH';\n        try {\n          await navigator.clipboard.writeText(link);\n          copyState.textContent = 'Link copied!';\n          copyState.classList.add('text-sky-700');\n          setTimeout(()=>{ copyState.textContent = link; copyState.classList.remove('text-sky-700'); }, 2000);\n        } catch {\n          copyState.textContent = link + ' (copy manually)';\n        }\n      });\n    "}]}],"doctype":"<!DOCTYPE html>","head":{"type":"head","name":"Head","components":[{"tagName":"meta","name":"Meta Charset","void":true,"attributes":{"charset":"utf-8"}},{"tagName":"meta","name":"Meta Viewport","void":true,"attributes":{"name":"viewport","content":"width=device-width, initial-scale=1"}},{"tagName":"title","type":"text","name":"Title","components":[{"type":"textnode","content":"Australia Photo Album"}]},{"tagName":"link","name":"Google Fonts","void":true,"attributes":{"href":"https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600&family=Plus+Jakarta+Sans:wght@600;700&display=swap","rel":"stylesheet"}},{"type":"script","name":"Tailwind Loader","attributes":{"src":"https://cdn.tailwindcss.com"}},{"type":"script","name":"Tailwind Config","components":[{"type":"textnode","content":"tailwind.config={theme:{extend:{fontFamily:{display:[\"Plus Jakarta Sans\",\"Inter\",\"ui-sans-serif\",\"system-ui\"],body:[\"Inter\",\"system-ui\",\"-apple-system\",\"Segoe UI\",\"Roboto\",\"Helvetica\",\"Arial\"]}},screens:{\"lg\":{\"max\":\"992px\"},\"sm\":{\"max\":\"480px\"}}}};"}]}]},"docEl":{"tagName":"html","attributes":{"lang":"en"}}},"id":"xPBHvknP292UT7w0","minHeight":"500px"}],"id":"FnKmWMNyqS12TXuzZB0R5laFH"},{"name":"Taiwan Photo Album","frames":[{"component":{"type":"wrapper","name":"Body Root","classes":["gjs-t-body","body-root"],"attributes":{"id":"iirahyh"},"components":[{"tagName":"header","name":"Navbar","classes":["navbar"],"components":[{"name":"Navbar Container","classes":["navbar-container"],"components":[{"