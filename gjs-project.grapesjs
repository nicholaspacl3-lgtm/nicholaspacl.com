ata-gjs-name]\").forEach(el => {\n          if (el.closest(\"section\")) {\n            el.classList.add(\"opacity-0\", \"translate-y-4\");\n            observer.observe(el);\n          }\n        });\n\n        document.querySelectorAll(\".gjs-t-button\").forEach(btn => {\n          btn.addEventListener(\"click\", () => {\n            btn.classList.add(\"ring-2\", \"ring-sky-300\");\n            setTimeout(() => btn.classList.remove(\"ring-2\", \"ring-sky-300\"), 200);\n          });\n        });\n      })();\n    "}]}],"doctype":"<!DOCTYPE html>","head":{"type":"head","components":[{"tagName":"title","type":"text","components":[{"type":"textnode","content":"Electronics & IoT Entrepreneur | Portfolio & Blog"}]},{"tagName":"meta","void":true,"attributes":{"charset":"utf-8"}},{"tagName":"meta","void":true,"attributes":{"name":"viewport","content":"width=device-width, initial-scale=1"}},{"tagName":"meta","void":true,"attributes":{"name":"robots","content":"index,follow"}},{"tagName":"meta","void":true,"attributes":{"name":"generator","content":"GrapesJS Studio"}},{"tagName":"link","void":true,"attributes":{"href":"https://fonts.googleapis.com","rel":"preconnect"}},{"tagName":"link","void":true,"attributes":{"href":"https://fonts.gstatic.com","rel":"preconnect","crossorigin":""}},{"tagName":"link","void":true,"attributes":{"href":"https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700&family=Space+Grotesk:wght@400;500;700&display=swap","rel":"stylesheet"}}]},"docEl":{"tagName":"html","attributes":{"lang":"en"}}},"id":"aiAkULBCBnKIobUD","minHeight":"500px"}],"id":"JuZtnkXuaaiN7xDt"},{"name":"About Me","frames":[{"component":{"type":"wrapper","name":"Body","classes":["gjs-t-body","body"],"attributes":{"id":"iqcwgg"},"components":[{"tagName":"header","name":"Header Navbar","classes":["header-navbar"],"attributes":{"id":"iae538"},"components":[{"name":"Navbar Container","classes":["navbar-container"],"attributes":{"id":"i0mo51"},"components":[{"type":"link","n